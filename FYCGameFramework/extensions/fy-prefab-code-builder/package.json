{
    "package_version": 2,
    "version": "1.0.0",
    "name": "fy-prefab-code-builder",
    "description": "根据预制，自动生成Model、View、Controller代码",
    "main": "./dist/main.js",
    "dependencies": {
        "vue": "^3.1.4",
        "fs-extra": "^10.0.0"
    },
    "devDependencies": {
        "@types/node": "^16.0.1",
        "@types/fs-extra": "^9.0.5",
        "typescript": "^4.3.4"
    },
    "contributions": {
        "menu": [
            {
                "path": "i18n:menu.extension/ForyunTools",
                "label": "构建预制代码",
                "message": "prefab-build"
            }
        ],
        "messages": {
            "prefab-build": {
                "methods": [
                    "prefabBuild"
                ]
            }
        },
        "shortcuts": [
            {
                "message": "prefab-build"
            }
        ]
    },
    "author": "Foryun",
    "editor": ">=3.3.0",
    "scripts": {
        "build": "tsc -b",
        "watch": "tsc -w"
    }
}
